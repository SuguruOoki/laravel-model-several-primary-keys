{
    "name": "laravel-several-primary-key/laravel-several-primary-key",
    "description": "",
    "keywords": [
        "laravel-several-primary-key",
        "laravel-several-primary-key"
    ],
    "homepage": "https://github.com/laravel-several-primary-key/laravel-several-primary-key",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Suguru Ohki",
            "email": "ohki_suguru@sue-san.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelSeveralPrimaryKey\\LaravelSeveralPrimaryKey\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelSeveralPrimaryKey\\LaravelSeveralPrimaryKey\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelSeveralPrimaryKey\\LaravelSeveralPrimaryKey\\LaravelSeveralPrimaryKeyServiceProvider"
            ],
            "aliases": {
                "LaravelSeveralPrimaryKey": "LaravelSeveralPrimaryKey\\LaravelSeveralPrimaryKey\\LaravelSeveralPrimaryKeyFacade"
            }
        }
    }
}
